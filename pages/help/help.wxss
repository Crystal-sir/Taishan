}

.text {
  margin: auto 15px;
  width: 13%;
}

.text image {
  width: 70rpx;
  height: 70rpx;
}

.controls {
  position: relative;
  top: 65%;
  left: 85%;
  /* display: flex; */
}

.controls .img {
  margin-top: 5px;
  width: 80rpx;
  height: 80rpx;
}

.full {
  top: 82%;
}

.chatroom{
  display: flex;
  
  flex-direction: column;
  
  width: 110rpx;
  height: 100rpx;
  
  position:absolute;
  top: 10rpx;
  right:1rpx;
}

