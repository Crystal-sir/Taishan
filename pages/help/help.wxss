.top-swich {

  display: flex;
  justify-content: space-between;
  flex-direction: column;
  align-items: center;
  width: 125rpx;
  height: 700rpx;
  border-radius: 15px;
  position:absolute;
  top: 120rpx;
  right:1rpx;
  border-bottom-left-radius: 5px;
}


::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

