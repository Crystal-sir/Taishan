<!--pages/look/taohua/taohua.wxml-->
<view>
<block wx:for-items="{{list}}" wx:key="{{item.id}}">
  <view>
  <view class="head">{{item.name}}</view>
  <view class="picture">
  <image src='cloud://yin-5g0cfopc68ce8576.7969-yin-5g0cfopc68ce8576-1306543725/桃花峪.jpg'></image>
  </view>
</view>

<view>
    <view class="content">
    <view class="incontent">
    <text class="write">
    <text class="title">路线概述</text>
    {{item.describe}}
    </text>
    </view>
    <view class="incontent">
      <text class="write">
    <text class="title">沿途景点</text>
    {{item.path}}
    </text>
    </view>
    <view class="incontent">
    <text class="write">
    <text class="title">温馨提示</text>
    {{item.method}}
    </text>
    </view>
    </view>
   </view>
  </block>
</view>
